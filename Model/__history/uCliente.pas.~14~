unit uCliente;

interface

type
  TModelCliente = Class

  private
  FId          :Integer;
  FNome        :String;
  FTelefone    :String;

  public
  property Id         :Integer read FId       write FId;
  property Nome       :String  read FNome     write FNome;
  property Telefone   :String  read FTelefone write FNome;

  End;
implementation

end.
